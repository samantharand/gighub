<%- include('../partials/header.ejs') %>
	
	<h2> ALL EVENTS</h2>

	<div class="cardGrid">
		
		<% events.forEach((event) => { %>	
			
			<div class="card">
				
				<a href="/events/<%=event._id%>"><img src="<%=event.eventPhoto%>" class="indexPhoto"></a>
				<a href="/events/<%=event._id%>"><%= event.name %></a>		
			
			</div>
		
		<%})%>
	
	</div>

