<%- include('../partials/header.ejs') %>
	
	<h2>BANDS</h2>
	<%= message %>


	<div class="cardGrid">

		<% bands.forEach(band => { %>

			<div class="photoCard">	
				<a href="/bands/<%=band.id%>"><img class="indexPhoto" src="<%= band.bandPhoto %>"></a>
				<a href="/bands/<%=band.id%>"><%=band.name%></a>
			</div>
		
		<% }) %>
	
	</div>


<%- include('../partials/footer.ejs') %>
