<%- include('../partials/header.ejs') %>
<h2>NEW BAND</h2>
<form method="POST" action="/bands">
	<fieldset>
		<div>
			<label for='name'>*BAND NAME:</label>
			<input type="name" name="name" id="name" placeholder="BAND NAME"
			/>
		</div>
		<div>
			<label for="bandPhoto">PHOTO:</label>
			<input type="text" name="bandPhoto" id="bandPhoto" placeholder="PHOTO OF BAND"
			/>
		</div>
		<div>
			<label for="formed">FORMED:</label>
			<input type="number" name="formed" id="formed" placeholder="formed"
			/>
		</div>
		<div>
			<label for="genre">GENRE:</label>
			<input type="text" name="genre" id="genre" placeholder="WHAT KIND OF MUSIC"
			/>
		</div>
		<div>
			<label for='song'>A SONG:</label>
			<input type="text" name="song" id="song" placeholder="song"
			/>
		</div>
	</fieldset>
	<button>ADD BAND!</button>
</form>


<%- include('../partials/footer.ejs') %>
